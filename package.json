{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "nodemon --exec ts-node --files server/index.ts",
    "build:server": "tsc -p tsconfig.json",
    "start:server": "node dist/server/index.js",
    "seed": "set NODE_OPTIONS=--loader ts-node/esm && node --experimental-modules auction-seeder/seed.ts",
    "seed:drop": "set NODE_OPTIONS=--loader ts-node/esm && node --experimental-modules auction-seeder/seed.ts --drop"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "lucide-react": "^0.468.0",
    "mongoose": "^7.8.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.68",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.x.x",
    "typescript": "^4.x.x",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
